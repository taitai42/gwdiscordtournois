# Token de votre bot Discord (obtenu sur https://discord.com/developers/applications)
DISCORD_TOKEN=votre_token_ici

# ID de l'application Discord (Application ID depuis le portail développeur)
APPLICATION_ID=votre_application_id_ici

# ID du canal où poster les messages
CHANNEL_ID=id_du_canal_ici

# Heure à laquelle poster le message du matin (format HH:MM, heure française)
MORNING_POST_TIME=09:00
